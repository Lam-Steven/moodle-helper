{
  "manifest_version": 2,
  "name": "Chrome Extension",
  "version": "0.1",
  "web_accessible_resources": ["pictures/*.jpg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.jpg"
  }
}
